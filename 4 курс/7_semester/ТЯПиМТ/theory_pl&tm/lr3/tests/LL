K0 -> REQ_S INIT | INIT | e                                     

S -> REQ_S | e											  
REQ_S -> \s S                                          

INIT -> namespace REQ_S ID_FUNC S { K0 } K0 | MOD_TYPE ID_FUNC S ( PARAMS0 ) S BODY K0
									  
BODY -> ; | { S }                                    

MOD_TYPE -> MOD_TYPE_NONVOID | VOID    
MOD -> unsigned | signed                
MOD_TYPE_NONVOID -> MOD REQ_S TYPE | TYPE  
VOID -> void                                          
TYPE -> int REQ_S INT|double REQ_S DOUBLE|float REQ_S|char REQ_S|long REQ_S LONG|short REQ_S SHORT
INT -> LENGTH REQ_S | e                              
DOUBLE -> long REQ_S | e                               
LONG -> LONGTYPES REQ_S | e                           
SHORT -> int REQ_S | e 


MOD_TYPE_NONVOID_PARAMS -> MOD REQ_S TYPE_PARAMS | TYPE_PARAMS
TYPE_PARAMS -> int INT_PARAMS | double DOUBLE_PARAMS|float|char|long LONG_PARAMS|short SHORT_PARAMS
INT_PARAMS -> REQ_S INT_REQ_S | e 
INT_REQ_S -> LENGTH | ID_NON   

DOUBLE_PARAMS -> REQ_S DOUBLE_REQ_S  | e   
DOUBLE_REQ_S -> long | ID_NON      

LONG_PARAMS -> REQ_S LONG_REQ_S  | e 
LONG_REQ_S -> LONGTYPES  | ID_NON  

SHORT_PARAMS -> REQ_S SHORT_REQ_S | e      
SHORT_REQ_S -> int  | ID_NON                         

LENGTH -> long | short                               
LONGTYPES -> int | double  

                            

ID_FUNC -> [a-zA-Z_] TAIL_ID_FUNC
TAIL_ID_FUNC -> [a-zA-Z0-9_] TAIL_ID_FUNC
ID -> [a-zA-Z_] INNERID                               
INNERID -> [a-zA-Z0-9_] INNERID | S ARRAY             
ARRAY -> [ S OP S ] S ARRAY | e                       
OP -> [1-9] OP1                                        
OP1 -> [0-9] OP1 | e                                  

PARAMS0 -> REQ_S PARAMS | PARAMS | e
PARAMS -> MOD_TYPE_NONVOID_PARAMS | VOID S
PARAMS_NONVOID -> MOD_TYPE_NONVOID_PARAMS    
ID_NON -> ID COMMA_NON | COMMA_NON               
COMMA_NON ->  , S TAIL | e
TAIL -> PARAMS_NONVOID | ...                                   
